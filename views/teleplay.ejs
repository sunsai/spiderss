          <tr>
            <td><%= i + 1 %></td>
            <td><%= data[i].tvName %></td>
            <td><%= data[i].genre %></td>
            <td><%= data[i].youku %></td>
            <td><%= data[i].tudou %></td>
            <td><%= data[i].qiyi %></td>
            <td><%= data[i].sohu %></td>
            <td><%= data[i].tengxun %></td>
