<script>
  $(function () {
    $('#navbar li').click(function () {
      $('#navbar li').removeClass('active');
      $(this).addClass('active');
    })
    $('#leftnav li').click(function () {
      $('#leftnav li').css({'background':''}).removeClass('active');
      $(this).css({'background': '#337ab7'}).addClass('active');
    })
  })
</script>
